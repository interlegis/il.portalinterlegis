<object name="portal_css" meta_type="Stylesheets Registry">

 <stylesheet id="++resource++jquery-ui-themes/sunburst/jqueryui.css" rendering="link" />

 <!-- Deactivate bootstrap stylesheets that come with collective.js.bootstrap -->
 <stylesheet
    id="++resource++collective.js.bootstrap/css/bootstrap.min.css"
    enabled="0"/>
