    id="++resource++collective.js.bootstrap/css/bootstrap-responsive.min.css"
    enabled="0"/>

 <!-- Activate ours instead -->
 <stylesheet
    id="++theme++il.portalinterlegis/bootstrap/css/bootstrap.min.css"
    applyPrefix="True"
    media="" rel="stylesheet" rendering="link"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression=""/>

 <stylesheet title=""
    id="++theme++il.portalinterlegis/css/bootstrap-responsive.min.css"
    applyPrefix="True"
    media="" rel="stylesheet" rendering="link"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression=""/>

</object>


